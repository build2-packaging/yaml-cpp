#pragma once

// The numeric version format is AAAAABBBBBCCCCCDDDE where:
//
// AAAAA - major version number
// BBBBB - minor version number
// CCCCC - bugfix version number
// DDD   - alpha / beta (DDD + 500) version number
// E     - final (0) / snapshot (1)
//
// When DDDE is not 0, 1 is subtracted from AAAAABBBBBCCCCC. For example:
//
// Version      AAAAABBBBBCCCCCDDDE
//
// 0.1.0        0000000001000000000
// 0.1.2        0000000001000020000
// 1.2.3        0000100002000030000
// 2.2.0-a.1    0000200001999990010
// 3.0.0-b.2    0000299999999995020
// 2.2.0-a.1.z  0000200001999990011
//
#define YAML_CPP_VERSION       $yaml_cpp.version.project_number$ULL
#define YAML_CPP_VERSION_STR   "$yaml_cpp.version.project$"
#define YAML_CPP_VERSION_ID    "$yaml_cpp.version.project_id$"
#define YAML_CPP_VERSION_FULL  "$yaml_cpp.version$"

#define YAML_CPP_VERSION_MAJOR $yaml_cpp.version.major$
#define YAML_CPP_VERSION_MINOR $yaml_cpp.version.minor$
#define YAML_CPP_VERSION_PATCH $yaml_cpp.version.patch$

#define YAML_CPP_PRE_RELEASE   $yaml_cpp.version.pre_release$

#define YAML_CPP_SNAPSHOT_SN   $yaml_cpp.version.snapshot_sn$ULL
#define YAML_CPP_SNAPSHOT_ID   "$yaml_cpp.version.snapshot_id$"
